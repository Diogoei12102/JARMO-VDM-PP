class Tests

operations
	
	protected assertTrue: bool ==> ()
	assertTrue(arg) == 
		if not arg then (
  			IO`print("False expression")
  	)
  	else (
  		IO`print("True expression");
  		return )
		) 
	post arg

	protected assertEqual: ? * ? ==> ()
	assertEqual(expected, actual) == 
		if expected <> actual then (
  			IO`print("Actual value (");
  			IO`print(actual); 
  			IO`print(") different from expected (");
  			IO`print(expected);
  			IO`println(")\n")
		)
	post expected = actual

end Tests